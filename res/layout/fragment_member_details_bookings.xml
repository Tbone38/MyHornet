<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" 
    android:id="@+id/pageLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:padding="7dp"
    tools:context=".MemberDetails" >  
	<LinearLayout 
	    android:id="@+id/booking_list_heading"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal">
	   
	</LinearLayout>
    <LinearLayout
        android:id="@+id/booking_list"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_below="@id/booking_list_heading"
        android:orientation="vertical"
        android:padding="4dp" >
        
    </LinearLayout>	
   	 <include 
	    layout="@layout/actions_member_details_bookings"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/booking_list"/>
</RelativeLayout>